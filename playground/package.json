{
    "name": "@adyen/adyen-platform-experience-web-playground",
    "private": true,
    "type": "module",
    "keywords": [
        "adyen",
        "adyen-fp",
        "fp",
        "finance",
        "platforms",
        "adyen-for-platforms",
        "components"
    ],
    "version": "1.0.0",
    "license": "MIT",
    "repository": "github:Adyen/adyen-platform-experience-web",
    "bugs": {
        "url": "https://support.adyen.com/"
    },
    "scripts": {
        "vite": "vite dev",
        "start:local": "vite dev --mode local-env --config vite.config.ts",
        "start:loop": "vite dev --mode loop --config vite.config.ts",
        "start:test": "vite dev --mode development --config vite.config.ts",
        "start:beta": "vite dev --mode beta --config vite.config.ts",
        "build:demo": "vite build --mode demo --config vite.config.ts",
        "demo": "npm run build:demo && npm run demo:serve",
        "demo:serve": "vite preview --mode demo --config vite.config.ts",
        "demo:serve:e2e": "cross-env E2E_TEST=true vite preview --mode demo --config vite.config.ts",
        "type-check": "tsc --watch --preserveWatchOutput",
        "types:watch": "npm run type-check -- --watch",
        "start:mock": "vite dev --mode mocked --config vite.config.ts",
        "start:mock:e2e": "cross-env E2E_TEST=true npm run start:mock",
        "format": "prettier --write \"src/**/*.{ts,js,css, scss, html}\"",
        "storybook:local": "vite-node --mode local-env ../.storybook/runStorybook.ts",
        "storybook:test": "vite-node --mode development ../.storybook/runStorybook.ts",
        "storybook:loop": "vite-node --mode loop ../.storybook/runStorybook.ts",
        "storybook:beta": "vite-node --mode beta ../.storybook/runStorybook.ts",
        "storybook:mocked": "vite-node ../.storybook/runStorybook.ts --mode mocked",
        "storybook:build": "vite-node ../.storybook/buildStorybook.ts",
        "storybook:demo": "vite-node ../.storybook/buildStorybook.ts --mode demo && npm run demo:serve"
    },
    "devDependencies": {
        "@adyen/adyen-platform-experience-web-mocks": "^1.0.0",
        "@preact/preset-vite": "^2.5.0",
        "@storybook/addon-a11y": "^7.0.4",
        "@storybook/addon-essentials": "^7.0.4",
        "@storybook/core-server": "^7.0.4",
        "@storybook/manager-api": "^7.0.4",
        "@storybook/preact": "^7.0.4",
        "@storybook/preact-vite": "^7.0.4",
        "@storybook/types": "^7.0.4",
        "autoprefixer": "^9.8.8",
        "cross-env": "^7.0.3",
        "css-loader": "^4.3.0",
        "dotenv": "^16.0.3",
        "file-loader": "^6.2.0",
        "msw": "^1.3.2",
        "postcss": "^8.3.11",
        "prettier": "^2.4.1",
        "sass": "^1.43.4",
        "sass-loader": "^12.3.0",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.2.6",
        "typescript": "^4.9.4",
        "vite": "^4.2.1",
        "vite-node": "^0.30.1"
    },
    "dependencies": {
        "@adyen/adyen-platform-experience-web": "1.0.1",
        "preact": "^10.13.2"
    },
    "msw": {
        "workerDirectory": "public"
    }
}
