        "_exporter_id": "30937394"
    },
    "item": [
        {
            "name": "Authe",
            "item": [
                {
                    "name": "Create Session",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n    \"allowOrigin\": \"http://localhost\",\n    \"reference\": \"asd\",\n    \"product\": \"platform\",\n    \"policy\": {\n        \"resources\": [\n            {\n                \"type\": \"accountHolder\",\n                \"accountHolderId\": \"{{YOUR_ACCOUNT_HOLDER_ID}}\"\n            }\n        ],\n        \"roles\": [\n            \"{{ROLE_TO_ASSIGN_1}}\" // ...list of as many roles as desired\n        ]\n    }\n}"
                        },
                        "url": {
                            "raw": "{{autheBaseUrl}}/sessions",
                            "host": ["{{autheBaseUrl}}"],
                            "path": ["sessions"]
                        }
                    },
                    "response": []
                }
            ],
            "event": [
                {
                    "listen": "prerequest",
                    "script": {
                        "type": "text/javascript",
                        "exec": [""]
                    }
                },
                {
                    "listen": "test",
                    "script": {
                        "type": "text/javascript",
                        "exec": [""]
                    }
                }
            ]
        },
