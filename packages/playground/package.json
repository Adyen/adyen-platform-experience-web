        "css-loader": "^4.3.0",
        "dotenv": "^16.0.3",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "postcss": "^8.3.11",
        "prettier": "^2.4.1",
        "sass": "^1.43.4",
        "sass-loader": "^12.3.0",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.2.6",
        "typescript": "^4.9.4",
        "vite": "^4.2.1",
        "vite-node": "^0.30.1",
        "webpack": "^5.61.0",
