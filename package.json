{
    "name": "@pabloai/adyen-efp-main",
    "version": "1.0.0",
    "keywords": [
        "adyen",
        "adyen-efp",
        "efp",
        "finance",
        "platforms",
        "adyen-for-platforms",
        "components"
    ],
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "private": true,
    "workspaces": [
        "./packages/*"
    ],
    "scripts": {
        "start": "concurrently --kill-others-on-fail \"npm run start -w @pabloai/adyen-efp\" \"npm run start -w @pabloai/adyen-efp-playground\" --names \"lib,playground\"",
        "build": "npm --workspace @pabloai/adyen-efp build",
        "lint": "npm --workspace @pabloai/adyen-efp lint",
        "test": "npm --workspace @pabloai/adyen-efp test",
        "test:watch": "npm --workspace @pabloai/adyen-efp test:watch",
        "test:coverage": "npm --workspace @pabloai/adyen-efp test:coverage",
        "test:e2e": "npm build && npm workspace @pabloai/adyen-efp-e2e test:e2e",
        "type-check": "npm workspace @pabloai/adyen-efp type-check"
    },
    "dependencies": {
        "concurrently": "^7.0.0"
    }
}
